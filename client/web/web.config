<configuration>
  <system.webServer>  
    <rewrite>  
        <rules>  
          <rule name="Redirect requests to default azurewebsites domain" stopProcessing="true">
            <match url="(.*)" />  
            <conditions logicalGrouping="MatchAny">
              <add input="{HTTP_HOST}" pattern="^pyright-playground\.azurewebsites\.net$" />
            </conditions>
            <action type="Redirect" url="https://pyright-play.net/{R:0}" />  
          </rule>  
        </rules>  
    </rewrite>  
  </system.webServer>  
</configuration>
